        <div class="icon-text">
            <h3>@ViewBag.TongDoanhThu VNĐ</h3>
            <p>Doanh Thu</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="new-users">
        <div class="icon">
            <i class="user1"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.User</h3>
